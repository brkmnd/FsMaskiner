var Stack1 = function(){
    var s = {};
    var len = 0;
    var pub = {};
    };
var Stack = function(){
    var tail = {
        next:null,
        prev:null,
        v:null
        };
    var head = tail;
    var retO = {};
    var len = 0;
    retO.push = function(v){
        var n0;
        if(head.prev !== null){
            n0 = head.prev;
            n0.v = v;
            }
        else {
            n0 = {};
            n0.next = head;
            n0.prev = null;
            n0.v = v;
            }
        head.prev = n0;
        head = n0;
        len++;
        };
    retO.pop = function(){
        if(head.next === null){
            return null;
            }
        var v = head.v;
        head = head.next;
        len--;
        return v;
        };
    retO.popN = function(n,f){
        var h0 = head;
        while(n > 0 && h0.next !== null){
            f(retO.pop());
            h0 = head;
            n--;
            }
        };
    retO.peek = function(){
        if(head.next === null){
            return null;
            }
        return head.v;
        };
    retO.foreach = function(f){
        var h0 = head;
        while(h0.next !== null){
            f(h0.v);
            h0 = h0.next;
            }
        };
    retO.count = function(){
        return len;
        };
    return retO;
    };
var t = {
    shift:function(d){
        return {type:"shift",v:d};
        },
    reduce:function(d){
        return {type:"reduce",v:d};
        },
    accept:function(d){
        return {type:"accept",v:null};
        },
    error:function(msg){
        return {type:"error",v:msg};
        },
    none:function(){
        return {type:"none",v:null};
        },
    some:function(v){
        return {type:"some",v:v}
        }
    };
var isSome = function(m){
    return m.t === "some";
    };
var isNone = function(m){
    return m.t === "none";
    };
var _token = function(m,o){
    //m.tv = token val
    //m.t = some/none
    //m.v = value if some, else null
    if(o[m.tv] !== undefined){
        return o[m.tv](m.tv,m.v);
        }
    };
var addToken2tree = function(tree,node){
    _token(node,{
        "int":function(tokenV,capV){ tree.push({ntype:"int",nval:capV}); },
        "id":function(tokenV,capV){ tree.push({ntype:"id",nval:capV}); }
    });
    return tree;
    };
